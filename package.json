{
  "name": "ember-frost-notifier",
  "version": "4.3.4",
  "description": "An ember service and component for managing application notifications",
  "frostGuideDirectory": "ui-components/indicator-controls/notifications",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": "https://github.com/ciena-frost/ember-frost-notifier.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Sophy Pal (https://github.com/sophypal)",
  "contributors": [
    "Roxanne Panthaky (https://github.com/rox163)",
    "Steven Glanzer (https://github.com/sglanzer)",
    "David Fortin (https://github.com/dafortin)",
    "Matthew Dahl (https://github.com/sandersky)"
  ],
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.5.0",
    "ember-cli": "~2.11.0",
    "ember-cli-chai": "0.3.2",
    "ember-cli-code-coverage": "^0.3.10",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-ecosystem-installer": "^2.0.30",
    "ember-cli-ember-ecosystem": "0.4.0",
    "ember-cli-htmlbars-inline-precompile": "0.3.6",
    "ember-cli-inject-live-reload": "^1.6.1",
    "ember-cli-mocha": "0.13.2",
    "ember-cli-test-loader": "^1.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-cli-visual-acceptance": "^2.2.0",
    "ember-computed-decorators": "0.3.0",
    "ember-concurrency": "0.7.19",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-elsewhere": "0.4.1",
    "ember-export-application-global": "^1.1.1",
    "ember-frost-core": "^1.21.0",
    "ember-hook": "^1.4.1",
    "ember-load-initializers": "0.6.3",
    "ember-prop-types": "^3.10.2",
    "ember-resolver": "^2.1.1",
    "ember-sinon": "0.6.0",
    "ember-spread": "^1.1.1",
    "ember-test-utils": "^5.1.1",
    "ember-truth-helpers": "^1.3.0",
    "loader.js": "^4.1.0"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.5",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-sass": "^5.1.0",
    "ember-frost-core": "^1.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "ember-frost-icon-pack": {
    "name": "frost-notifier"
  },
  "pr-bumper": {
    "coverage": 88
  }
}
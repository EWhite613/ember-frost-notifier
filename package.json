{
  "name": "ember-frost-notifier",
  "version": "4.3.7",
  "description": "An ember service and component for managing application notifications",
  "frostGuideDirectory": "ui-components/indicator-controls/notifications",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "check-coverage": "pr-bumper check-coverage",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- ember test"
  },
  "repository": "https://github.com/ciena-frost/ember-frost-notifier.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Sophy Pal (https://github.com/sophypal)",
  "contributors": [
    "Roxanne Panthaky (https://github.com/rox163)",
    "Steven Glanzer (https://github.com/sglanzer)",
    "David Fortin (https://github.com/dafortin)",
    "Matthew Dahl (https://github.com/sandersky)"
  ],
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "2.16.2",
    "ember-cli-chai": "0.4.3",
    "ember-cli-code-coverage": "^0.3.10",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-frost-blueprints": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-concurrency": "~0.8.10",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-elsewhere": "1.0.2",
    "ember-export-application-global": "^2.0.0",
    "ember-frost-core": "^2.0.0",
    "ember-frost-test": "^2.1.2",
    "ember-hook": "^1.4.2",
    "ember-load-initializers": "^1.0.0",
    "ember-prop-types": "^4.0.1",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.16.0",
    "ember-spread": "^2.0.1",
    "ember-test-utils": "^6.0.0",
    "ember-truth-helpers": "^2.0.0",
    "eslint-plugin-ember": "^4.5.0",
    "loader.js": "^4.2.3",
    "pr-bumper": "^2.0.1",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-sass": "7.0.0",
    "ember-decorators": "1.3.0"
  },
  "ember-addon": {
    "after": [
      "ember-decorators"
    ],
    "configPath": "tests/dummy/config"
  },
  "ember-frost-icon-pack": {
    "name": "frost-notifier"
  },
  "pr-bumper": {
    "coverage": 88
  }
}
